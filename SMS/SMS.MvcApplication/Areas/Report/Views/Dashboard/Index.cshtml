@{
    ViewBag.Title = "Index";
}

<div id="content">
    <!--[if lt IE 9]><script type="text/javascript" src="~/Scripts/flot/excanvas.min.js"></script><![endif]-->
    <script type="text/javascript" src="~/Scripts/flot/jquery.flot.min.js"></script>
    
    Welcome to report dashboard page
    <div class="row">
        <div class="two columns"></div>
        <div class="eight columns" id="line-chart-div" style="height: 500px;"></div>
        <div class="two columns"></div>
    </div>
    
    <script type="text/javascript">

        var d1 = [];
        for (var i = 0; i < 14; i += 0.5) {
            d1.push([i, Math.sin(i)]);
        }

        var d2 = [[0, 3], [4, 8], [8, 5], [9, 13]];

        // A null signifies separate line segments

        var d3 = [[0, 12], [7, 12], null, [7, 2.5], [12, 2.5]];

        $.plot("#line-chart-div", [d1, d2, d3]);

	</script>
</div>